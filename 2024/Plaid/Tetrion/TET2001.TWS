GenerationPhase {
  GenerationTime ((.8-((level-1)*.007))^(level-1))/20
  PieceStartingLocation (4, 0)

  GenerationBeginScript {
    set pf.d2 to 1
    set pf.d1 to 0
  }
}

FallingPhase {
  FallSpeed (.8-((level-1)*.007))^(level-1)
  SoftDropSpeed ((.8-((level-1)*.007))^(level-1))/20
  HardDropSpeed 0.0001
  MoveTolerance 1

  FallingEndScript {
    set pf.d7 to 1
  }
}

LockPhase {
  MovementRestartsClock
  Duration 0.5
}

PatternPhase {
  PatternBeginScript {}

  PatternEndScript {
    set pf.d7 to 0
    if pf.d6 = 0
      if ph.d8
        if ph.d9 - pf.d0 * 10 != 20
          set ph.d0 to 1
        end
        if ph.d4
          set ph.d0 to 1
        end
        if ph.d0
          lose
        else
          set pf.d6 to 1
        end
      end
    end
  }
}

IteratePhase {
  PrimaryIterator {
    IteratorExpression pf.d6 = 1
    IteratorTags 5

    MinoIteratorScript {
      mutate mino
    }
  }

  SecondaryIterator {
    IteratorExpression pf.d6 = 1
    IteratorTags 8

    MinoIteratorScript {
      set mino to "Arneb"
    }
  }

  IterateEndScript {
    if pf.d6 = 1
      set pf.d6 to 2
    end
  }
}

EliminatePhase {
  FallSpeed 0.05

  LineCollapsedScript {
    set ph.d0 to ph.d0 + 1
  }

  StartFallingScript {
    if ph.d0 = 1
      line color (109,0,23)
    end
    if ph.d0 = 2
      line color (39,0,87)
    end
    if ph.d0 = 3
      line color (116,55,0)
    end
    if ph.d0 >= 4
      line color (0,91,56)
    end
  }

  GroupLandedScript {
    sound "FX_LNE01"
  }

  EliminateEndScript {
    if pf.d6 = 0
      set pf.d1 to pf.d1 + ph.d0
      if ph.d0 = 1
        sound "VO_LINE01"
        sound "FX_MTRS1"
        message "Single"
      end
      if ph.d0 = 2
        sound "VO_LINE02"
        sound "FX_MTRS2"
        message "Double"
      end
      if ph.d0 = 3
        sound "VO_LINE03"
        sound "FX_MTRS3"
        message "Triple"
      end
      if ph.d0 = 4
        sound "VO_TETRS"
        sound "FX_MTRS4"
        message "Tetris!"
      end
      if ph.d0 = 5
        sound "VO_TETRS"
        sound "FX_MTRS5"
        message "Pentris!!"
      end
    end
  }
}

SolidifyPhase {}

GravityPhase {
  GravityEnabled
  FallSpeed 0.05

  GroupLandedScript {
    sound "FX_LOCK"
  }
