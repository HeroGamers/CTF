Pattern {
  Name "Tianyi"
  Tags 8
  Method Search
  SearchType Horizontal
  SearchCount 10

  PatternBeginScript {
    set ph.d6 to 0
  }

  PatternMinoScript {
    if ph.d8
      if mino.d0
        set ph.d6 to 1 - ph.d6
      end
      if mino.d3
        if mino.d2 - 1 != ph.d6
          set ph.d0 to 1
        end
      end
      if mino.d2
        set ph.d6 to mino.d2 - 1
      end
      if mino.d1
        set pl.d2 to ((pl.d2 + mino.d0) * 17) % 97
        if mino.d0 = 0
          mutate mino
          mutate mino
        end
        mutate mino
        mutate mino
        mutate mino
        if ph.d6
          mutate mino
        end
      end
    end
  }
}

