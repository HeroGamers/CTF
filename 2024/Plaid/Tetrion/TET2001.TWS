CompletionPhase {
  CompletionBeginScript {
    set pl.d0 to pf.d1
    if pl.d1 = pf.d5
      set pl.d9 to pl.d9 + 1
    else
      set pl.d9 to 1
    end
    set pl.d1 to pf.d5
    set pl.d3 to pl.d5
    set pl.d8 to pf.d8
    if pf.d6 = 2
      if level <= 1
        sound "VO_WOW"
      else
        if level <= 3
          sound "VO_THTGREAT"
        else
          if level <= 5
            sound "VO_VRYGOOD"
          else
            if level <= 7
              sound "VO_EXLNT"
            else
              if level <= 9
                sound "VO_AMAZING"
              else
                sound "VO_BRILLIANT"
              end
            end
          end
        end
      end
      sound "FX_MTRS6"
      message "LEVEL UP!"
      next level
    end
  }
}

