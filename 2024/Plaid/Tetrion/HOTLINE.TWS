Pattern {
  Name "Suhail"
  Tags 2
  Method Search
  SearchType Horizontal
  SearchCount 1

  PatternMinoScript {
    if pf.d7
      set ph.d3 to 1
      set ph.d2 to mino.d9
      if ph.d2 % 11
        if pl.d8 + 1 != ph.d2 % 11
          set ph.d3 to 0
        end
      end
      set ph.d2 to (ph.d2 - ph.d2 % 11) / 11
      if ph.d2 % 5
        if pl.d9 != ph.d2 % 5
          set ph.d3 to 0
        end
      end
      set ph.d2 to (ph.d2 - ph.d2 % 5) / 5
      if ph.d2 % 3
        if pl.d3 + 1 != ph.d2 % 3
          set ph.d3 to 0
        end
      end
      set ph.d2 to (ph.d2 - ph.d2 % 3) / 3
      if ph.d2 % 6
        if pl.d0 + 1 != ph.d2 % 6
          set ph.d3 to 0
        end
      end
      set ph.d2 to (ph.d2 - ph.d2 % 6) / 6
      if ph.d2
        if pl.d1 != ph.d2
          set ph.d3 to 0
        end
      end
      if ph.d3
        pattern "Altair" at mino.x, mino.y
      end
    end
  }
}

