  LevelTrash "Terebellum"

  LevelStartScript {
    set pf.d0 to 5
  }
}

Level {
  Name "Coma Berenices"
  LevelTrash "Surtur"

  LevelStartScript {
    set pf.d0 to 5
  }
}

Level {
  Name "Deneb Algedi"
  LevelTrash "Calypso"

  LevelStartScript {
    set pf.d0 to 5
  }
}

Level {
  Name "Nihal"
  LevelTrash "Sagarmatha"

  LevelStartScript {
    set pf.d0 to 6
  }
}

Level {
  Name "Wangshu"
  LevelTrash "Kavian"

  LevelStartScript {
    set pf.d0 to 6
  }
}

GenerationPhase {
  GenerationTime ((.8-((level-1)*.007))^(level-1))/20
  PieceStartingLocation (4, 0)

  GenerationBeginScript {
    set pf.d2 to 1
    set pf.d1 to 0
  }
}

FallingPhase {
  FallSpeed (.8-((level-1)*.007))^(level-1)
  SoftDropSpeed ((.8-((level-1)*.007))^(level-1))/20
  HardDropSpeed 0.0001
  MoveTolerance 1

  FallingEndScript {
    set pf.d7 to 1
  }
}

LockPhase {
  MovementRestartsClock
  Duration 0.5
}

PatternPhase {
  PatternBeginScript {}

  PatternEndScript {
    set pf.d7 to 0
    if pf.d6 = 0
      if ph.d8
        if ph.d9 - pf.d0 * 10 != 20
          set ph.d0 to 1
        end
        if ph.d4
          set ph.d0 to 1
        end
        if ph.d0
          lose
        else
          set pf.d6 to 1
        end
      end
    end
  }
}

IteratePhase {
  PrimaryIterator {
    IteratorExpression pf.d6 = 1
    IteratorTags 5

    MinoIteratorScript {
      mutate mino
    }
  }

  SecondaryIterator {
    IteratorExpression pf.d6 = 1
    IteratorTags 8

    MinoIteratorScript {
      set mino to "Arneb"
    }
  }

  IterateEndScript {
    if pf.d6 = 1
      set pf.d6 to 2
    end
  }
}

EliminatePhase {
  FallSpeed 0.05

  LineCollapsedScript {
    set ph.d0 to ph.d0 + 1
  }

  StartFallingScript {
    if ph.d0 = 1
      line color (109,0,23)
    end
    if ph.d0 = 2
      line color (39,0,87)
    end
    if ph.d0 = 3
      line color (116,55,0)
    end
    if ph.d0 >= 4
      line color (0,91,56)
    end
  }

  GroupLandedScript {
    sound "FX_LNE01"
  }

  EliminateEndScript {
    if pf.d6 = 0
      set pf.d1 to pf.d1 + ph.d0
      if ph.d0 = 1
        sound "VO_LINE01"
        sound "FX_MTRS1"
        message "Single"
      end
      if ph.d0 = 2
        sound "VO_LINE02"
        sound "FX_MTRS2"
        message "Double"
      end
      if ph.d0 = 3
        sound "VO_LINE03"
        sound "FX_MTRS3"
        message "Triple"
      end
      if ph.d0 = 4
        sound "VO_TETRS"
        sound "FX_MTRS4"
        message "Tetris!"
      end
      if ph.d0 = 5
        sound "VO_TETRS"
        sound "FX_MTRS5"
        message "Pentris!!"
      end
    end
  }
}

SolidifyPhase {}

GravityPhase {
  GravityEnabled
  FallSpeed 0.05

  GroupLandedScript {
    sound "FX_LOCK"
  }
}

CompletionPhase {
  CompletionBeginScript {
    set pl.d0 to pf.d1
    if pl.d1 = pf.d5
      set pl.d9 to pl.d9 + 1
    else
      set pl.d9 to 1
    end
    set pl.d1 to pf.d5
    set pl.d3 to pl.d5
    set pl.d8 to pf.d8
    if pf.d6 = 2
      if level <= 1
        sound "VO_WOW"
      else
        if level <= 3
          sound "VO_THTGREAT"
        else
          if level <= 5
            sound "VO_VRYGOOD"
          else
            if level <= 7
              sound "VO_EXLNT"
            else
              if level <= 9
                sound "VO_AMAZING"
              else
                sound "VO_BRILLIANT"
              end
            end
          end
        end
      end
      sound "FX_MTRS6"
      message "LEVEL UP!"
      next level
    end
  }
}

EventScripts {
  GameStartScript {
    set st2.d7 to rank
    sound "FX_LEVEL"
    set ghost to 1
    set st0.d0 to 0
    set st0.d1 to 0
    set st0.d2 to 0
    set st0.d3 to 0
    set st0.d4 to 0
    set st0.d5 to 0
    set st0.d7 to 0
    set st0.d8 to 0
    set st0.d9 to 0
    set st1.d0 to 0
    set ph.d8 to 0
    set ph.d7 to 0
    line color (30,30,30)
  }

  LevelStartScript {
    if level < 3
      mino intensity low
    else
      if level < 6
        mino intensity medium
      else
        mino intensity high
      end
    end
    if level = 1
      set pf.d9 to 1
    end
    if level = 2
      set pf.d9 to 21
    end
    if level = 3
      set pf.d9 to 84
    end
    if level = 4
      set pf.d9 to 74
    end
    if level = 5
      set pf.d9 to 57
    end
    if level = 6
      set pf.d9 to 11
    end
    if level = 7
      set pf.d9 to 44
    end
    if level = 8
      set pf.d9 to 63
    end
    if level = 9
      set pf.d9 to 14
    end
    if level = 10
      set pf.d9 to 55
    end
    set goal to 0
    set ph.d8 to 0
    set ph.d7 to 0
    set pf.d1 to 0
    set pf.d6 to 0
    set pl.d0 to 0
    set pl.d1 to 0
    set pl.d3 to 0
    set pf.d5 to 0
    set pl.d5 to 0
  }

  LevelEndScript {
    if level = 1
      set st0.d0 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 2
      set st0.d1 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 3
      set st0.d2 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 4
      set st0.d3 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 5
      set st0.d4 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 6
      set st0.d5 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 7
      set st0.d6 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 8
      set st0.d7 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 9
      set st0.d8 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level = 10
      set st0.d9 to 31 + ((pl.d2 + pf.d9) % 97)
    end
    if level >= 10
      win
    end
  }

  MoveSuccessfulScript {
    sound "FX_FALL"
    if piece.id = 7
      shape "Grumium" at piece.x, piece.y
      set pl.d6 to 8 * result
      shape "Metis" at piece.x, piece.y
      set pl.d6 to pl.d6 + 4 * result
      shape "Tupi" at piece.x, piece.y
      set pl.d6 to pl.d6 + 2 * result
      shape "Perseus" at piece.x, piece.y
      set pl.d6 to pl.d6 + result
    else
      if piece.id != 6
        shape "Phoebe" at piece.x, piece.y
        set pl.d6 to 8 * result
        shape "Montuno" at piece.x, piece.y
        set pl.d6 to pl.d6 + 4 * result
        shape "Noquisi" at piece.x, piece.y
        set pl.d6 to pl.d6 + 2 * result
        shape "Al Minliar al Asad" at piece.x, piece.y
        set pl.d6 to pl.d6 + result
      end
    end
  }

  MoveFailedScript {
    sound "FX_LND01"
  }

  RotateSuccessfulScript {
    sound "FX_RTA01"
    if piece.id = 7
      shape "Grumium" at piece.x, piece.y
      set pl.d6 to 8 * result
      shape "Metis" at piece.x, piece.y
      set pl.d6 to pl.d6 + 4 * result
      shape "Tupi" at piece.x, piece.y
      set pl.d6 to pl.d6 + 2 * result
      shape "Perseus" at piece.x, piece.y
      set pl.d6 to pl.d6 + result
    else
      if piece.id != 6
        shape "Phoebe" at piece.x, piece.y
        set pl.d6 to 8 * result
        shape "Montuno" at piece.x, piece.y
        set pl.d6 to pl.d6 + 4 * result
        shape "Noquisi" at piece.x, piece.y
        set pl.d6 to pl.d6 + 2 * result
        shape "Al Minliar al Asad" at piece.x, piece.y
        set pl.d6 to pl.d6 + result
      end
    end
  }

  RotateFailedScript {
    sound "FX_LND01"
  }

  HardDropStartScript {
    sound "FX_HRDDP"
    set pf.d2 to 0
  }

  HardDropStepScript {
    if piece.id = 7
      shape "Grumium" at piece.x, piece.y
      set pl.d6 to 8 * result
      shape "Metis" at piece.x, piece.y
      set pl.d6 to pl.d6 + 4 * result
      shape "Tupi" at piece.x, piece.y
      set pl.d6 to pl.d6 + 2 * result
      shape "Perseus" at piece.x, piece.y
      set pl.d6 to pl.d6 + result
    else
      if piece.id != 6
        shape "Phoebe" at piece.x, piece.y
        set pl.d6 to 8 * result
        shape "Montuno" at piece.x, piece.y
        set pl.d6 to pl.d6 + 4 * result
        shape "Noquisi" at piece.x, piece.y
        set pl.d6 to pl.d6 + 2 * result
        shape "Al Minliar al Asad" at piece.x, piece.y
        set pl.d6 to pl.d6 + result
      end
    end
  }

  SoftDropStepScript {
    sound "FX_FALL"
    if piece.id = 7
      shape "Grumium" at piece.x, piece.y
      set pl.d6 to 8 * result
      shape "Metis" at piece.x, piece.y
      set pl.d6 to pl.d6 + 4 * result
      shape "Tupi" at piece.x, piece.y
      set pl.d6 to pl.d6 + 2 * result
      shape "Perseus" at piece.x, piece.y
      set pl.d6 to pl.d6 + result
    else
      if piece.id != 6
        shape "Phoebe" at piece.x, piece.y
        set pl.d6 to 8 * result
        shape "Montuno" at piece.x, piece.y
        set pl.d6 to pl.d6 + 4 * result
        shape "Noquisi" at piece.x, piece.y
        set pl.d6 to pl.d6 + 2 * result
        shape "Al Minliar al Asad" at piece.x, piece.y
        set pl.d6 to pl.d6 + result
      end
    end
  }

  FallStepScript {
    sound "FX_FALL"
    if piece.id = 7
      shape "Grumium" at piece.x, piece.y
      set pl.d6 to 8 * result
      shape "Metis" at piece.x, piece.y
      set pl.d6 to pl.d6 + 4 * result
      shape "Tupi" at piece.x, piece.y
      set pl.d6 to pl.d6 + 2 * result
      shape "Perseus" at piece.x, piece.y
      set pl.d6 to pl.d6 + result
    else
      if piece.id != 6
        shape "Phoebe" at piece.x, piece.y
        set pl.d6 to 8 * result
        shape "Montuno" at piece.x, piece.y
        set pl.d6 to pl.d6 + 4 * result
        shape "Noquisi" at piece.x, piece.y
        set pl.d6 to pl.d6 + 2 * result
        shape "Al Minliar al Asad" at piece.x, piece.y
        set pl.d6 to pl.d6 + result
      end
    end
  }

  TouchdownScript {
    if pf.d2
      sound "FX_LND01"
    end
  }

  LockdownScript {
    set pf.d5 to piece.id
    if pf.d2
      sound "FX_LCK01"
    end
    if piece.id = 7
      shape "Perseus" at piece.x, piece.y
      if result * (1 - pl.d6 % 2)
        set pl.d4 to 1
      end
      set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
      shape "Tupi" at piece.x, piece.y
      if result * (1 - pl.d6 % 2)
        set pl.d4 to 2
      end
      set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
      shape "Metis" at piece.x, piece.y
      if result * (1 - pl.d6 % 2)
        set pl.d4 to 3
      end
      set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
      shape "Grumium" at piece.x, piece.y
      if result * (1 - pl.d6 % 2)
        set pl.d4 to 4
      end
    else
      if piece.id != 6
        shape "Al Minliar al Asad" at piece.x, piece.y
        if result * (1 - pl.d6 % 2)
          set pl.d4 to 1
        end
        set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
        shape "Noquisi" at piece.x, piece.y
        if result * (1 - pl.d6 % 2)
          set pl.d4 to 2
        end
        set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
        shape "Montuno" at piece.x, piece.y
        if result * (1 - pl.d6 % 2)
          set pl.d4 to 3
        end
        set pl.d6 to (pl.d6 - pl.d6 % 2) / 2
        shape "Phoebe" at piece.x, piece.y
        if result * (1 - pl.d6 % 2)
          set pl.d4 to 4
        end
      end
    end
    set pl.d5 to 0
    if piece.id = 1
      if pl.d4 = 2
        shape "Rocinante" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 4
        shape "Scorpius" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 2
      if pl.d4 = 1
        shape "Tarvos" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 3
        shape "Tondra" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 3
      if pl.d4 = 1
        shape "Situla" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 2
        shape "Meissa" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 3
        shape "Albiorix" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 4
        shape "Kaewkosin" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 4
      if pl.d4 = 1
        shape "Chaophraya" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 2
        shape "Laomedeia" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 3
        shape "Morava" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 4
        shape "Leo" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 5
      if pl.d4 = 1
        shape "Citala" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 2
        shape "Su" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 3
        shape "Noifasui" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 4
        shape "Poerava" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 7
      if pl.d4 = 1
        shape "Pandora" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 2
        shape "Themisto" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 3
        shape "Juliet" at piece.x, piece.y
        set pl.d5 to result
      end
      if pl.d4 = 4
        shape "Fafnir" at piece.x, piece.y
        set pl.d5 to result
      end
    end
    if piece.id = 6
      shape "Hoggar" at piece.x, piece.y
      set pl.d5 to result
      shape "Eyeke" at piece.x, piece.y
      set pl.d5 to pl.d5 + result
    end
    set pf.d8 to 9 - (game time - game time % 1) % 10
    if pl.d5
      sound "FX_FUS02"
    end
  }
}

